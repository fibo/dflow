{
  "name": "dflow",
  "description": "is a minimal Dataflow programming engine",
  "version": "0.30.0",
  "homepage": "http://g14n.info/dflow",
  "type": "module",
  "exports": "./dflow.js",
  "funding": "https://github.com/sponsors/fibo",
  "engines": {
    "node": ">=14",
    "deno": ">=1.10"
  },
  "author": {
    "name": "Gianluca Casati",
    "url": "http://g14n.info"
  },
  "license": "MIT",
  "scripts": {
    "bundle": "deno bundle dflow.ts | esbuild --minify --target=safari11 > dflow.js",
    "example:abstract": "deno run examples/abstract.js",
    "example:context": "deno run examples/context.ts",
    "example:execution-report": "deno run examples/execution-report.ts",
    "example:usage": "deno run examples/usage.js",
    "fmt": "deno fmt --ignore=./dflow.js",
    "generate_types": "tsc --project ./tsconfig.declarations.json; mv engine.d.ts index.d.ts; echo 'export const catalog: DflowNodesCatalog;' >> index.d.ts",
    "lint": "deno lint --ignore=./dflow.js",
    "postversion": "git push origin v${npm_package_version}; git push origin main; npm publish",
    "start": "serve",
    "test": "deno test --unstable --importmap=import_map.json"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/fibo/dflow.git"
  },
  "keywords": [
    "dataflow",
    "visual"
  ],
  "dependencies": {}
}
