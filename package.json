{
  "name": "dflow",
  "description": "is a minimal Dataflow programming engine",
  "version": "0.28.0",
  "homepage": "http://g14n.info/dflow",
  "type": "module",
  "exports": "./dflow.js",
  "types": "engine.ts",
  "funding": "https://github.com/sponsors/fibo",
  "engines": {
    "node": ">=14",
    "deno": ">=1.10"
  },
  "author": {
    "name": "Gianluca Casati",
    "url": "http://g14n.info"
  },
  "license": "MIT",
  "scripts": {
    "bundle:dflow": "deno bundle dflow.ts | esbuild --minify --target=safari11 > dflow.js",
    "bundle:engine": "deno bundle engine.ts | esbuild --minify --target=safari11 > engine.js",
    "example:abstract": "deno run examples/abstract.js",
    "example:usage": "deno run examples/usage.js",
    "fmt": "deno fmt --ignore=./dflow.js,./engine.js",
    "lint": "deno lint --ignore=./dflow.js,./engine.js",
    "start": "serve",
    "test": "deno test --unstable --importmap=import_map.json"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/fibo/dflow.git"
  },
  "keywords": [
    "dataflow",
    "visual"
  ],
  "dependencies": {}
}
