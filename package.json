{
  "name": "dflow",
  "version": "0.5.0",
  "description": "Dataflow programming",
  "main": "index.js",
  "scripts": {
    "gh-pages": "jekyll build && git commit -a && git subtree --prefix gh-pages/_site push origin gh-pages",
    "test": "mocha --bail --require should --reporter min --recursive",
    "build": "browserify -s $npm_package_name -e index.js -o dist/${npm_package_name}.js",
    "jshint": "jshint -c .jshintrc src/*.js"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git://github.com/fibo/dflow.git"
  },
  "keywords": [
    "dataflow",
    "visual"
  ],
  "author": {
    "name": "fibo",
    "url": "http://g14n.info"
  },
  "license": [
    {
      "type": "MIT",
      "url": "http://g14n.info/mit-license"
    }
  ],
  "testling": {
    "harness": "mocha-tdd",
    "files": "test/*.js",
    "browsers": [
      "ie/8..latest",
      "chrome/latest",
      "firefox/latest"
    ]
  },
  "homepage": "http://g14n.info/dflow",
  "devDependencies": {
    "mocha": "~2.1.0",
    "should": "~5.0.1",
    "browserify": "~9.0.3"
  }
}
